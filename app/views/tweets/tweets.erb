
<h1>Welcome, to Fwitter </h1>
<h1> A list of All Tweets</h1>
  <ol>
    <% @tweets.each do |tweet| %>

        <li><%= tweet.content %>


          </li>
        <% if tweet.user_id == session[:user_id] %>
            <a href="/tweets/<%= tweet.id %>/edit"> Edit</a>
            <form method="POST" action='/tweets/<%= tweet.id %>/delete'>
              <input name="_method" type="hidden" value="delete">
              <input type="submit" value="Delete Tweet" id="Delete Tweet">
            </form>
        <% end %>

    <% end %>

  </ol>

<h2><a href='/tweets/new'> Add a Tweet </a></h2>
